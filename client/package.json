{
  "private": true,
  "scripts": {
    "clean": "rimraf static dist",
    "build": "npm run clean && npm run lint && npm run _build:build",
    "build-dev": "npm run _build:clean && npm run lint && npm run _build:build-dev",
    "dev-server": "webpack-dev-server --host 0.0.0.0 --port 9005 --hot --hot-only --inline --history-api-fallback --progress --open",
    "dist-server": "npm run _dist-server:open & npm run _dist-server:serve",
    "lint": "eslint src",
    "_build:build": "cross-env NODE_ENV='production' webpack -p",
    "_build:build-dev": "cross-env NODE_ENV='development' webpack -d",
    "_dist-server:serve": "pushstate-server ./ 9005 /index.html",
    "_dist-server:open": "wait-on http://localhost:9005/ && opn http://localhost:9005/"
  },
  "dependencies": {
    "todomvc-app-css": "^2.0.0",
    "todomvc-common": "^1.0.0",
    "liveresource": "^0.2.0-beta.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-discard-module-references": "^1.0.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.0",
    "babel-runtime": "^6.9.2",
    "copyfiles": "^1.0.0",
    "core-js": "^2.4.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.4.1",
    "file-loader": "^0.9.0",
    "opn-cli": "^3.1.0",
    "pushstate-server": "^1.10.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "wait-on": "^1.5.2",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-server": "^2.1.0-beta.0",
    "whatwg-fetch": "^1.0.0"
  }
}
